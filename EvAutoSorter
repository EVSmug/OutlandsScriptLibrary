//Jewels
createlist 'JewelTypes'
@pushlist 'JewelTypes' 0xf13 //Ruby
@pushlist 'JewelTypes' 0xf25 //Amber
@pushlist 'JewelTypes' 0xf19 //Sapphire
@pushlist 'JewelTypes' 0xf26 //Diamond
@pushlist 'JewelTypes' 0xf2d //Tourmaline
@pushlist 'JewelTypes' 0xf10 //Emerald
@pushlist 'JewelTypes' 0xf21 //Star Sapphire
@pushlist 'JewelTypes' 0xf16 //Amethyst
@pushlist 'JewelTypes' 0xf15 //Citrine
for 0 to 'ItemTypes'
  while @findtype 'ItemTypes[]' 'any' 'backpack'
    @clearjournal
    pause 100
    moveitem 'found' 'ArmorBox' 0 0 0 'any'
    pause 500
  endwhile
endfor
for 0 to 'WeaponTypes'
  while @findtype 'WeaponTypes[]' 'any' 'backpack'
    @clearjournal
    pause 100
    moveitem 'found' 'WeaponBox' 0 0 0 'any'
    pause 500
  endwhile
endfor
for 0 to 'JewelTypes'
  while @findtype 'JewelTypes[]' 'any' 'backpack'
    @clearjournal
    pause 100
    moveitem 'found' 'JewelBox' 0 0 0 'any'
    pause 500
  endwhile
endfor
sysmsg 'Thanks EV you are my hero'
msg 'Thanks EV you are my hero'
