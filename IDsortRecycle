@pushlist 'ItemTypes' 0x13fd //HeavyXbow
@pushlist 'ItemTypes' 0xf50  //Xbow
@pushlist 'ItemTypes' 0x13b2 //bow
for 0 to 'ItemTypes'
  while @findtype 'ItemTypes[]' 'any' 'bagtosort'
    @clearjournal
    pause 100
    useskill 'Item Identification'
    waitfortarget 2000
    target! found
    pause 2000
    if @injournal 'not certain' 'system'
      continue
    endif
    //change 'ruin' to whichever property you are looking to keep
    if @injournal 'Vanq' 'system'
      moveitem 'found' 'goodshit'
      pause 500
    elseif @injournal 'power' 'system'
      moveitem 'found' 'goodshit'
      pause 500
    elseif @injournal 'supremely' 'system'
      moveitem 'found' 'goodshit'
      pause 500
    elseif @injournal 'slaying' 'system'
      moveitem 'found' 'goodshit'
      pause 500
    elseif @injournal 'Exceedingly' 'system'
      moveitem 'found' 'goodshit'
      pause 500
    else
      ignoreobject 'found'
    endif
  endwhile
endfor
//recycle
usetype 0x1034 0 'backpack'
pause 500
waitforgump 0x38920abd 15000
replygump 0x38920abd 3
waitforgump 0x38920abd 15000
waitfortarget 15000
target! 'self'
waitforgump 0x38920abd 15000
waitforgump 0x38920abd 15000
